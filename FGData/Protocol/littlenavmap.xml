<?xml version="1.0"?>

<!-- property list for vfrflight -->
<PropertyList>
    <generic>
        <output>
            <line_separator>newline</line_separator>
            <var_separator>;</var_separator>

            <chunk>
                <name>time-gmt</name>
                <format>%s</format>
                <type>string</type>
                <node>/sim/time/gmt</node>
            </chunk>
             <chunk>
                <name>local-offset</name>
                <type>integer</type>
                <format>%d</format>
                <node>/sim/time/local-offset</node>
            </chunk>
            <chunk>
                <name>altitudeAboveGroundFt = 0.f</name>
                <type>float</type>
                <format>%f</format>
                <node>/position/altitude-agl-ft</node>
            </chunk>
            <chunk>
                <name>groundAltitudeFt = 0.f</name>
                <format>%f</format>
                <type>float</type>
                <node>/position/ground-elev-ft</node>
            </chunk>           
            <chunk>
                <name>windSpeedKts = 0.f</name>
                <format>%f</format>
                <type>float</type>
                <node>/environment/wind-speed-kt</node>
            </chunk>
            <chunk>
                <name>windDirectionDegT = 0.f</name>
                <format>%f</format>
                <type>float</type>
                <node>/environment/wind-from-heading-deg</node>
            </chunk>
            <chunk>
                <name>ambientTemperatureCelsius = 0.f</name>
                <format>%f</format>
                <type>float</type>
                <node>/environment/temperature-degc</node>
            </chunk>
            <!--totalAirTemperatureCelsius = 0.f,-->            
            <chunk>
                <name>seaLevelPressureMbar = 0.f</name>
                <format>%f</format>
                <type>float</type>
                <node>/environment/pressure-sea-level-inhg</node>
            </chunk>

            <!--pitotIcePercent = 0.f-->
            <!--structuralIcePercent = 0.f-->
            <!--airplaneTotalWeightLbs = 0.f,-->
            <!--airplaneMaxGrossWeightLbs = 0.f-->
            <!--airplaneEmptyWeightLbs = 0.f-->
            <chunk>
                <name>fuelTotalQuantityGallons = 0.f</name>
                <format>%f</format>
                <type>float</type>
                <node>/consumables/fuel/total-fuel-gals</node>
            </chunk>
            <chunk>
                <name>fuelTotalWeightLbs = 0.f</name>
                <format>%f</format>
                <type>float</type>
                <node>/consumables/fuel/total-fuel-lbs</node>
            </chunk>
            <!--fuelFlowPPH = 0.f-->
            <!--fuelFlowGPH = 0.f-->
            <chunk>
                <name>magVarDeg = 0.f</name>
                <format>%f</format>
                <type>float</type>
                <node>/environment/magnetic-variation-deg</node>
            </chunk>
            <chunk>
                <name>ambientVisibilityMeter = 0.f;</name>
                <format>%f</format>
                <type>float</type>
                <node>/environment/effective-visibility-m</node>
            </chunk>
            <chunk>
                <name>trackMagDeg = 0.f;</name>
                <format>%f</format>
                <type>float</type>
                <node>/orientation/track-magnetic-deg</node>
            </chunk>
            <chunk>
                <name>trackTrueDeg = 0.f;</name>
                <format>%f</format>
                <type>float</type>
                <node>/orientation/true-heading-deg</node>
            </chunk>
            
            <chunk>
                <name>airplaneTitle = ""</name>
                <format>%s</format>
                <type>string</type>
                <node>/sim/description</node>
            </chunk>
            <!--airplaneType = ""-->
            <chunk>
                <name>airplaneModel = ""</name>
                <format>%s</format>
                <type>string</type>
                <node>/sim/aircraft</node>
            </chunk>
            <chunk>
                <name>airplaneReg = ""</name>
                <format>%s</format>
                <type>string</type>
                <node>/sim/user/callsign</node>
            </chunk>
            <!--airplaneAirline = ""-->
            <!--airplaneFlightnumber = ""-->
            <!--fromIdent = ""-->
            <!--toIdent = ""-->

            <chunk>
                <name>atools::geo::Pos position</name>
                <type>float</type>
                <format>%.10f</format>
                <node>/position/latitude-deg</node>
            </chunk>
            <chunk>
                <name>atools::geo::Pos position</name>
                <type>float</type>
                <format>%.10f</format>
                <node>/position/longitude-deg</node>
            </chunk>            
            <chunk>
                <name>headingTrueDeg = 0.f</name>
                <format>%f</format>
                <type>float</type>
                <node>/orientation/heading-deg</node>
            </chunk>
            <chunk>
                <name>headingMagDeg = 0.f</name>
                <format>%f</format>
                <type>float</type>
                <node>/orientation/heading-magnetic-deg</node>
            </chunk>
            <chunk>
                <name>groundSpeedKts = 0.f</name>
                <format>%f</format>
                <type>float</type>
                <node>/velocities/groundspeed-kt</node>
            </chunk>
            <chunk>
                <name>indicatedAltitudeFt = 0.f</name>
                <format>%f</format>
                <type>float</type>
                <node>/instrumentation/altimeter/indicated-altitude-ft</node>
            </chunk>
            <chunk>
                <name>indicatedSpeedKts = 0.f</name>
                <format>%f</format>
                <type>float</type>
                <node>/instrumentation/airspeed-indicator/indicated-speed-kt</node>
            </chunk>
            <chunk>
                <name>trueAirspeedKts = 0.f</name>
                <format>%f</format>
                <type>float</type>
                <node>/instrumentation/airspeed-indicator/true-speed-kt</node>
            </chunk>
            <chunk>
                <name>machSpeed = 0.f</name>
                <format>%f</format>
                <type>float</type>
                <node>/instrumentation/airspeed-indicator/indicated-mach</node>
            </chunk>
            <chunk>
                <name>verticalSpeedFeetPerMin = 0.f</name>
                <format>%f</format>
                <type>float</type>
                <node>/instrumentation/vertical-speed-indicator/indicated-speed-fpm</node>
            </chunk>

            <!--modelRadiusFt = 0-->
            <!--wingSpanFt = 0-->
            <!--objectId = 0L-->

            <!--AircraftFlags flags = atools::fs::sc::NONE;-->
            <!--Category category;-->
            <!--EngineType engineType = atools::fs::sc::UNSUPPORTED;-->
            <!--quint8 numberOfEngines = 0;-->
            <!--bool debug = false;-->

        </output>

    </generic>

</PropertyList>
